{
  "author": "Florian Keller <github@floriankeller.de>",
  "dependencies": {},
  "description": "Open a GitHub repository in your browser",
  "devDependencies": {
    "@ffflorian/prettier-config": "0.0.3",
    "@semantic-release/changelog": "3.0.2",
    "@semantic-release/git": "7.0.8",
    "@types/node": "11.13.8",
    "husky": "2.1.0",
    "lint-staged": "8.1.5",
    "prettier": "1.17.0",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.3",
    "tslint": "5.16.0",
    "tslint-config-prettier": "1.18.0",
    "tslint-plugin-prettier": "2.0.1",
    "typescript": "3.4.5"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "file",
    "index",
    "fileindex",
    "tree"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ],
    "src/**/*.ts": [
      "tslint --project tsconfig.json --fix",
      "git add"
    ]
  },
  "main": "dist/index.js",
  "name": "@ffflorian/file-index",
  "prettier": "@ffflorian/prettier-config",
  "repository": "https://github.com/ffflorian/file-index.git",
  "scripts": {
    "build": "tsc",
    "clear": "rimraf dist",
    "dist": "yarn clear && yarn build",
    "lint": "yarn lint:ts && yarn lint:other",
    "lint:other": "yarn prettier --list-different",
    "lint:ts": "tslint --project tsconfig.json \"**/*.ts\"",
    "prettier": "prettier --write \"*.{json,md}\"",
    "release": "semantic-release",
    "test": "exit 0"
  },
  "version": "0.0.1"
}
